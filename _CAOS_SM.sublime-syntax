# See http://www.sublimetext.com/docs/3/syntax.html
file_extensions:
  - cos
name: 'Sea Monkeys'

first_line_match: '\*\s*([Cc][Aa]?[Oo][Ss]\s*|[@]?[Vv][Aa][Rr][Ii][Aa][Nn][Tt])(\(\s*[Ss][Mm]\s*\)|\s*[:]\s*[Ss][Mm])'
scope: source.caos
contexts:
  main:

    - match: ','
      scope: invalid.illegal.caos

    - match: '\s*\*.*$'
      scope: comment.line.caos

    - match: '([_][Cc][Dd][_])\s*([Ee][Jj][Cc][Tt])'
      captures:
